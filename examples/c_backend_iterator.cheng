import cheng/stdlib/bootstrap/seqs

iterator items(values: int32[]): int32 =
    for i in 0..<len values:
        yield values[i]

fn sumValues(values: int32[]): int32 =
    var total: int32 = 0
    for v in items(values):
        total = total + v
    return total

fn main(): int32 =
    var nums: int32[] 
    seqAdd(nums, 1)
    seqAdd(nums, 2)
    seqAdd(nums, 3)
    let total = sumValues(nums)
    if total == 6:
        return 0
    return 1
