type
    Pair =
        a: int32
        b: int32

var g_i32: int32 = 40
var g_i64: int64 = 2

fn main(): int32 =
    if g_i32 + int32(g_i64) != 42:
        return 1
    g_i32 = g_i32 + 1
    if g_i32 != 41:
        return 2

    var sumFor: int32 = 0
    for i in 0..<5:
        sumFor = sumFor + i
    if sumFor != 10:
        return 3

    var j: int32 = 0
    let __for_start_j_0 = j
    for __for_j_0 in __for_start_j_0..<5:
        j = __for_j_0
        j = j + 1
        if j == 3:
            continue
        sumFor = sumFor + j
    if sumFor != 22:
        return 4

    let bit0: int32 = (1 | 2) ^ 3
    if bit0 != 0:
        return 5
    let bit1: int32 = (1 << 3) - (8 >> 1)
    if bit1 != 4:
        return 6

    var x: int32 = 1
    let p: int64 = &x
    *p = 41
    if x != 41:
        return 7

    var pair: Pair = Pair(a: 2, b: 3)
    if pair.a + pair.b != 5:
        return 8
    pair.a = 4
    if pair.a + pair.b != 7:
        return 9

    puts("fullspec ok")
    return 0
