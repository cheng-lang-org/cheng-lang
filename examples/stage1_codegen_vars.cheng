# Stage1 codegen coverage:
# var / if / infix
fn main() =
    var x: int32 = 1
    var y: int32 = 2
    if x < y:
        echo "x<y"
    else:
        echo "x>=y"
    var sum: int64 = 0
    for i in 0..< 3:
        if i == 1:
            continue
        sum = sum + i
    if sum == 2:
        echo "sum=2"
    return 0
