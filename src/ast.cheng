type
    SourcePos =
        file: str
        line: int
        col: int
type
    NodeKind = enum nkModule
type
    Node = ref
        kind: NodeKind
        pos: SourcePos
        filename: str
        source: str
fn newModuleNode(filename: str, source: str): Node =
    var node: Node
    new(node)
    node.kind = nkModule
    var pos: SourcePos = SourcePos()
    pos.file = filename
    pos.line = 1
    pos.col = 1
    node.pos = pos
    node.filename = filename
    node.source = source node
