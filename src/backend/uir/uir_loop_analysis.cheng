# UIR loop analysis (phase-1): conservative loop detection placeholder.
import cheng/backend/uir/uir_types

type
    UirLoopInfo =
        loopCount: int32
        reducibleCount: int32

fn uirAnalyzeLoops(module: UirModule): UirLoopInfo =
    var info: UirLoopInfo
    info.loopCount = 0
    info.reducibleCount = 0
    if module == nil:
        return info
    return info
