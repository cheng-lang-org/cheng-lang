# RISC-V64 instruction selection: UIR -> Machine internal (MVP).
# Reuse the existing AArch64 lowering because produced Machine ops are
# architecture-neutral and encoded by the target object writer.
import std/result
import backend/uir/uir_types
import backend/machine/machine_types
import backend/machine/select_internal/aarch64_select

fn riscv64LowerModule(module: UirModule): Result[MachineModule] =
    return aarch64LowerModule(module)
