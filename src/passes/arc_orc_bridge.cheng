import cheng/runtime/option
import cheng/semantics/core
type
    ArcCallPos =
        file: str
        line: int
        column: int
type
    ArcCall =
        event: str
        owner: Option[int]
        ownerName: str
        borrower: Option[int]
        borrowerName: str
        borrowKind: Option[CaptureBorrowKind]
        pos: ArcCallPos
        scope: Option[str]
        thread: Option[str]
        inferenceKind: Option[CaptureBorrowKind]
        inferenceSource: Option[str]
        sharedAccess: Option[bool]
        requiresConfirmation: Option[bool]
        threadAffinity: str[]
fn buildArcCalls(borrowModule: BorrowModule): ArcCall[] =
    borrowModule []
