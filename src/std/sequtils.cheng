fn deduplicate(s: var T[]) =
    var out: T[]
    var i = 0
    let __for_guard_start_i_1 = i
    for __for_guard_i_1 in __for_guard_start_i_1..<len s:
        if ! (i < len s):
            break
        var exists = false
        var j = 0
        let __for_guard_start_j_2 = j
        for __for_guard_j_2 in __for_guard_start_j_2..<len out:
            if ! (j < len out):
                break
            if out[j] == s[i]:
                exists = true
                break
                j = j + 1
                if ! exists:
                    out.add s[i]
                    i = i + 1
                    s = out
