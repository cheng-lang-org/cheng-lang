# std/math (minimal, pure Cheng)
const Inf = 1.0
const NegInf = - Inf
fn round(x: float64): float64 =
    if x >= 0.0:
        float64(int64) x + 0.5
    else:
        float64(int64) x - 0.5
fn floor(x: float64): float64 =
    let i = int(x)
    let f = float64(i)
    if f > x:
        f - 1.0
    else:
        f
fn ceil(x: float64): float64 =
    let i = int(x)
    let f = float64(i)
    if f < x:
        f + 1.0
    else:
        f
