# std/monotimes (minimal, pure Cheng)
@importc("cheng_monotime_ns")
fn cheng_monotime_ns(): int64
type
    MonoTime =
        ns: int64
type
    Duration =
        ns: int64
fn getMonoTime(): MonoTime =
    var t: MonoTime
    t.ns = cheng_monotime_ns()
    return t
fn monoTimeNs(t: MonoTime): int64 =
    return t.ns
fn `-`(a, b: MonoTime): Duration =
    var d: Duration
    d.ns = a.ns - b.ns
    return d
fn inMilliseconds(d: Duration): int64 =
    d.ns / 1000000
