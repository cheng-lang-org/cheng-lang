# std/algorithm (minimal, pure Cheng)
fn sort(s: var str[]) =
    if len s <= 1:
        return
        var i = 0
        let __for_guard_start_i_1 = i
        for __for_guard_i_1 in __for_guard_start_i_1..<len s:
            if ! (i < len s):
                break
            var j = i + 1
            let __for_guard_start_j_1 = j
            for __for_guard_j_1 in __for_guard_start_j_1..<len s:
                if ! (j < len s):
                    break
                if cmp(s[j], s[i]) < 0:
                    let tmp = s[i]
                    s[i] = s[j]
                    s[j] = tmp
                    j = j + 1
                    i = i + 1
