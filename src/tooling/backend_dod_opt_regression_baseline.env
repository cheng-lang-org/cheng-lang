# P3PAR-03 quality gate baseline for backend.dod_opt_regression.
# Drift alerts are non-blocking by default (strict=0) and can be hardened in CI by setting strict=1.
BACKEND_DOD_OPT_BASELINE_VERSION=1

# Hard minimal benefit thresholds (blocking).
BACKEND_DOD_OPT_MIN_BENEFIT_OBJ_SIZE_DELTA=8
BACKEND_DOD_OPT_MIN_BENEFIT_NOALIAS_TOTAL_CHANGES=2
BACKEND_DOD_OPT_MIN_BENEFIT_NOALIAS_CHANGED_FUNCS=1

# Drift alert policy (non-blocking by default).
BACKEND_DOD_OPT_DRIFT_ALERT_MIN_RATIO_PCT=60
BACKEND_DOD_OPT_DRIFT_ALERT_STRICT=0

# Benefit baseline (for drift alert ratio comparisons).
BACKEND_DOD_OPT_BASELINE_BENEFIT_OBJ_SIZE_DELTA=24
BACKEND_DOD_OPT_BASELINE_BENEFIT_NOALIAS_TOTAL_CHANGES=2
BACKEND_DOD_OPT_BASELINE_BENEFIT_NOALIAS_CHANGED_FUNCS=1
