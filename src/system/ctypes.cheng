# Minimal `system/ctypes` shim for Cheng Stage1 (IDE compatibility).
type
    cchar = char
    cuchar = uint8
    cschar = int8
    cshort = int16
    cushort = uint16
    cint = int32
    cuint = uint32
    clonglong = int64
    culonglong = uint64
    csize_t = uint
    cfloat = float32
    cdouble = float64
    cstring = str
