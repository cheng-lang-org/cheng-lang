import web/runtime/abi

const
    WebLogInfo: int32 = 0
    WebLogWarn: int32 = 1
    WebLogError: int32 = 2

fn webLog(level: int32, msg: str) =
    if msg == nil:
        return
    host_log(level, msg, len(msg))

fn webInfo(msg: str) =
    webLog(WebLogInfo, msg)

fn webWarn(msg: str) =
    webLog(WebLogWarn, msg)

fn webError(msg: str) =
    webLog(WebLogError, msg)
