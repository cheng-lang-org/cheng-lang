import web/runtime/abi

fn webHistoryListen(handlerId: WebHandler) =
    if handlerId == 0:
        return
    host_history_listen(handlerId)

fn webHistoryUnlisten(handlerId: WebHandler) =
    if handlerId == 0:
        return
    host_history_unlisten(handlerId)

fn webHistoryPush(path: str) =
    if path == nil:
        return
    host_history_push(path, len(path))

fn webHistoryReplace(path: str) =
    if path == nil:
        return
    host_history_replace(path, len(path))
