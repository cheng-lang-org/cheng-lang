# Stage1: seq[Node] monomorphized (for bootstrap).
type
    seq_Node =
        len: int32
        cap: int32
        buffer: void*
fn newSeq_Node(len: int32, cap: int32): seq_Node =
    var seqInstance: seq_Node
    seqInstance.len = 0
    seqInstance.cap = cap
    if cap > 0:
        var size64: int64 = int64(cap) * sizeof Node
        var size32: int32 = int32(size64)
        seqInstance.buffer = alloc size32
    else:
        seqInstance.buffer = nil
        return seqInstance
fn get_Node(seqInst: seq_Node, i: int32): Node =
    var idx64: int64 = int64(i)
    var offset64: int64 = idx64 * sizeof Node
    var offset32: int32 = int32(offset64)
    var p: void* = ptr_add(seqInst.buffer, offset32)
    var pt: Node* = Node* p
    return *pt
fn addPtr_Node(seqInst: seq_Node*, val: Node) =
    if seqInst->len >= seqInst->cap:
        var newCap: int32 = 4
        if seqInst->cap != 0:
            newCap = seqInst->cap * 2
            seqInst->cap = newCap
            var cap64: int64 = int64(newCap)
            var size64: int64 = int64(cap64) * sizeof Node
            var size32: int32 = int32(size64)
            seqInst->buffer = realloc(seqInst->buffer, size32)
            var idx64: int64 = int64(seqInst->len)
            var offset64: int64 = int64(idx64) * sizeof Node
            var offset32: int32 = int32(offset64)
            var p: void* = ptr_add(seqInst->buffer, offset32)
            var pt: Node* = Node* p
            *pt = val
            seqInst->len = seqInst->len + 1
