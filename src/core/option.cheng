module option
type
    Option[T] =
        has: bool
        value: T
fn Some[T](value: T): Option[T] =
    Option[T](has: true, value: value)
fn None[T](): Option[T] =
    var out: Option[T]
    out.has = false
    return out
fn some[T](value: T): Option[T] =
    Option[T](has: true, value: value)
fn none[T](t: typedesc[T]): Option[T] =
    t
    var out: Option[T]
    out.has = false
    return out
fn IsSome[T](opt: Option[T]): bool =
    opt.has
fn Get[T](opt: Option[T]): T =
    opt.value
