import runtime/option
import semantics/core

type
    OwnershipRewriteItem =
        annotation: str
        annotationKind: str

type
    OwnershipRewrite =
        scopePath: str
        lineIndex: int
        indent: str
        items: OwnershipRewriteItem[]

fn computeOwnershipRewrites(source: str): OwnershipRewrite[] =
    source
    []

fn applyOwnershipRewrites(source: str, rewrites: OwnershipRewrite[]): str =
    rewrites
    source

fn generateOwnershipPatch(path: str, source: str, rewrites: OwnershipRewrite[]): str =
    path
    source
    rewrites
    ""
