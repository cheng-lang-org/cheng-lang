import cheng/ast
import cheng/runtime/option

type
    SemSeverity = enum
        ssError
        ssWarning
        ssHint

type
    Diagnostic =
        severity: SemSeverity
        pos: SourcePos
        message: str

type
    SymbolKind = enum
        skModule
        skProc
        skMacro
        skParam
        skLet
        skVar
        skConst
        skType
        skLoop
        skCase

type
    SemSymbolInfo =
        name: str
        kind: SymbolKind
        storage: str
        mutable: bool
        exported: bool
        isAsync: bool
        typeName: str
        scopePath: str
        pos: SourcePos

type
    EffectKind = enum
        ekUnknown

type
    ProcEffectInfo =
        name: str
        scopePath: str
        pos: SourcePos
        effects: EffectKind[]

type
    TypeInfo =
        pos: SourcePos
        nodeKind: NodeKind
        typeName: str

type
    BorrowAnnotationSummary =
type
    BorrowNodeKind = enum
        bnUnknown

type
    BorrowNodeTrait = enum
        btUnknown

type
    BorrowNodeInfo =
        file: str
        line: int
        column: int

type
    BorrowNode =
        externalId: str
        name: str
        kind: BorrowNodeKind
        traits: BorrowNodeTrait[]
        thread: str
        info: BorrowNodeInfo

type
    BorrowModule =
        nodes: BorrowNode[]
