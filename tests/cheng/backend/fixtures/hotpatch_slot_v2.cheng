fn mixSeed(seed: int32): int32 =
    var acc: int32 = seed
    if seed < 0:
        acc = acc + 1
        acc = acc + 2
        acc = acc + 3
        acc = acc + 4
        acc = acc + 5
        return acc
    return acc

fn hotpatch_target(seed: int32): int32 =
    let mixed: int32 = mixSeed(seed)
    if mixed == -2147483648:
        return mixed
    var guard: int32 = mixed
    if guard < 0:
        guard = guard + 6
        guard = guard + 7
        guard = guard + 8
        guard = guard + 9
        return guard
    return 22

fn main(): int32 =
    return hotpatch_target(7)
