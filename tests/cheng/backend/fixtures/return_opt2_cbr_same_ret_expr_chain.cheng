fn branch_expr(flag: bool): int32 =
    if flag:
        let x: int32 = 1
        let y: int32 = x + 3
        return y
    else:
        let a: int32 = 1
        let b: int32 = a + 3
        return b

fn main(): int32 =
    let a: int32 = branch_expr(true)
    let b: int32 = branch_expr(false)
    return a + b - 8
