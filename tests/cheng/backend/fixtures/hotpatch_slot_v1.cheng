    let mixed: int32 = mixSeed(seed)
    if mixed == -2147483648:
        return mixed
    var guard: int32 = mixed
    if guard < 0:
        guard = guard + 13
        guard = guard + 14
        guard = guard + 15
        guard = guard + 16
        guard = guard + 17
        guard = guard + 18
        guard = guard + 19
        guard = guard + 20
        guard = guard + 21
        guard = guard + 22
        guard = guard + 23
        guard = guard + 24
        guard = guard + 25
        guard = guard + 26
        guard = guard + 27
        guard = guard + 28
        guard = guard + 29
        guard = guard + 30
        return guard
    return 11

fn main(): int32 =
