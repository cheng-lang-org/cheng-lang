import std/seqs
import std/strings

fn main(): int32 =
    var values: str[]
    add(values, "a")
    add(values, "b")
    values[0] = "c"
    values[1] = "d"

    if values.len != 2:
        return 1
    let v0: str = values[0]
    if len(v0) != 1 || v0[0] != 'c':
        return 2
    let v1: str = values[1]
    if len(v1) != 1 || v1[0] != 'd':
        return 3

    return 0
