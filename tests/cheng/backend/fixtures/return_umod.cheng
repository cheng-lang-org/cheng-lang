fn main(): int32 =
    var u: uint32 = -1
    if int32(u % uint32(2)) != 1:
        return 1
    if int32(uint32(10) % uint32(3)) != 1:
        return 2

    var u64: uint64 = -1
    if int64(u64 % uint64(2)) != 1:
        return 3
    if int64(uint64(10) % uint64(3)) != 1:
        return 4
    return 0
