fn branch_same_target(x: int32): int32 =
    if x == 0:
        return 5
    else:
        if x == 1:
            return 5
    return 3

fn main(): int32 =
    let a: int32 = branch_same_target(0)
    let b: int32 = branch_same_target(1)
    let c: int32 = branch_same_target(2)
    return a + b + c - 13
