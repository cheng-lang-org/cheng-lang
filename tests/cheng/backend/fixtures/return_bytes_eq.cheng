import std/rawbytes

fn main(): int32 =
    var a: Bytes = bytesAlloc 2
    var b: Bytes = bytesAlloc 2
    bytesSet(a, 0, 1)
    bytesSet(a, 1, 2)
    bytesSet(b, 0, 1)
    bytesSet(b, 1, 3)
    if a == b:
        return 1
    bytesSet(b, 1, 2)
    if a != b:
        return 2
    let s: str = $a
    if len(s) != 4:
        return 3
    return 0

