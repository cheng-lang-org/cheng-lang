fn main(): int32 =
    let a: float64 = 1.5
    let b: float64 = 2.25
    let c: float64 = a + b
    if !(c > 3.0):
        return 1
    if !(c >= 3.75):
        return 2
    if !(c < 4.0):
        return 3
    if !(c <= 3.75):
        return 4
    let d: float64 = c * 2.0
    let e: float64 = d / 3.0
    if !(e > 2.0):
        return 5
    let f: float64 = -e
    if !(f < 0.0):
        return 6
    return 0
