import std/tables
import std/json

fn main(): int32 =
    var t: tables.Table[int32] = tables.TableInit[int32] 8
    t["k"] = 9
    if t["k"] != 9:
        return 1
    let ts: str = $t
    if len(ts) == 0:
        return 2

    let obj: JsonNode = newJObject()
    obj["a"] = toJsonValue 1
    let v: JsonNode = obj["a"]
    if getInt(v) != 1:
        return 3
    let js: str = $obj
    if len(js) == 0:
        return 4
    if js[0] != '{':
        return 5
    return 0

