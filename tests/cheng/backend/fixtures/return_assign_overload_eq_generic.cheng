type Box[T] =
    value: T
    copied: bool

fn `=`[T](a: var Box[T], b: Box[T]) =
    a.value = b.value
    a.copied = true

fn main(): int32 =
    var x: Box[int32]
    x.value = 1
    x.copied = false
    var y: Box[int32]
    y.value = 7
    y.copied = false
    x = y
    if x.copied:
        return 0
    return 9
